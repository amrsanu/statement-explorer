{
    "version": 2,
    "installCommand": " npm rebuild && cp ./libx86/* node_modules/canvas/build/Release && npm install",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }
  