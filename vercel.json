{
    "version": 2,
    "name": "statement-explorer",
    "builds": [
        {
            "use": "npm" 
        },
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ],
    "env": {
      "CUSTOM_ENV_VAR": "your-custom-env-var-value"
    }
  }
  