{
    "version": 2,
    "name": "statement-explorer",
    "builds": [
        { 
            "src": "package.json", 
            "use": "@vercel/npm" }
    ],
    [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}